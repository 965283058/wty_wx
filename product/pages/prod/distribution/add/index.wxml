<view class="container">
     <cell>
        <text slot="label" class="cell-caption">送货方类型</text>
        <picker slot="content" value="{{po.wl}}" range="{{vo.wlList}}" class="cell-select">
            <text>{{vo.wlList[po.wl]}}</text>
        </picker>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">送货方编号</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入字母和数字"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">送货方简称</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入5个以内文字"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">送货方全称</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入12个以内文字"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">车排号</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入车牌号"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">联系人</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入联系人"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">联系电话</text>
        <input type="text" slot="content" class="cell-input" placeholder="请输入手机号"/>
    </cell>
    <cell>
        <text slot="label" class="cell-caption">状态</text>
        <picker slot="content" value="{{po.wl}}" range="{{vo.wlList}}" class="cell-select">
            <text>{{vo.wlList[po.wl]}}</text>
        </picker>
    </cell>
    <button class="cell-btn bg-green" size="default" bindtap="save">提交</button>
</view>

