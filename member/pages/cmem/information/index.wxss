.page {
  width: 100%;
  border-top: 2px solid #008e40;
  background: #fafafa;
}
.list{
  height: 130rpx;
  line-height: 130rpx;
}
.list{
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding-left: 22rpx;
  color:#666;
  font-size: 26rpx;
  border-bottom: 2rpx solid #dde0e0;
  position: relative;
}
.list__title {
  display: flex;
  justify-content: flex-start;
}
.logo{
  width: 150rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin: 15rpx 80rpx 0 0;
}
.logo1{
  width: 80rpx;
  height: 80rpx;
  margin: 30rpx 80rpx 0 0;
}

.logo {
  display: flex;
  justify-content: flex-end;
}
.logo:after {
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  border: 3rpx solid #8c8e91;
  border-left: 0;
  border-bottom: 0;
  transform: rotate(45deg);
  position: absolute;
  right: 49rpx;
  top: 60rpx;
  z-index: 1;
}
.list_bd{
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  border-bottom: 1rpx solid #e3e3e3;
  font-size: 26rpx;
  position: relative;
  color: #979797;
  background: #fff;
  text-align: center;
}

.list_title1 {
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  display: flex;
  justify-content: flex-start;
  margin-left: 20rpx;
}

.list_title1-right1 {
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  display: flex;
  justify-content: flex-end;
  padding: 0 60rpx 0 0;
  color: #3c3c3c;
}
.list_title1:after {
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  border: 3rpx solid #8c8e91;
  border-left: 0;
  border-bottom: 0;
  transform: rotate(45deg);
  position: absolute;
  right: 30rpx;
  top: 35rpx;
  z-index: 1;
}
.next__btn{
  background: #008E40;
  color: #fff;
  margin-top: 20rpx;
  width: 90%;
  font-size: 26rpx;
}