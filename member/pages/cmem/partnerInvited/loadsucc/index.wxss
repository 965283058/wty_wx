
.box{
  background: #f3f5f7;
}
.nav{
  display: flex;
  justify-content: space-between;
  padding-left: 30rpx;
  font-size: 22rpx;
  color: #666;
   height: 150rpx;
  background: #fff;
  border-bottom: 1rpx solid #e3e3e3;
}

.text__box_txt:first-child{
  margin-top: 30rpx;
}
.text__box_txt:nth-child(2){
  margin-top: -8rpx;
}
.big__text{
  font-size: 26rpx;
  margin-right: 12rpx;
}
.logo{
  display: flex;
  justify-content: flex-start;
  margin-top: 25rpx;
  width: 99rpx;
  height: 95rpx;
}
.nav__box{
  position: relative;
  width: 37%;
  display: flex;
  justify-content: space-between;
}
.nav__box:nth-child(2) {
  margin-left: -20rpx;
  width: 39%;
}
.text__box,.nickname{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin:0 10rpx;

  width:160rpx;
}
.nav__box .nickname{
  margin: 40rpx 0 0 2rpx;
  width: 180rpx;
}
.img{
  display: flex;
  justify-content: flex-end;
  right: 25rpx;
  top: 44rpx;
  position: relative;
}

.ewm{
  width:44rpx;
  height: 38rpx;
  margin-top: 12rpx;
}
.info__btn{
  width: 54rpx;
  height: 53rpx;
  margin: 5rpx 0 0 15rpx;
}
.num{
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  border-radius: 14rpx;
  color: #fff;
  background: #d83611;
  z-index: 999;
  font-size: 22rpx;
  text-align: center;
  margin: -4rpx 0 0 -7rpx;
}
.input__box{
  display: flex;
  width: 100%;
  align-items: center;
  position: relative;
}
.text{
  display: flex;
  justify-content: flex-start;
  font-size: 26rpx;
  color: #666;
  width: 100%;
  padding-left: 40rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #d8dddc;
}
.text1{
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  color: #666;
 margin-top: 20rpx;
  width: 100%;

}
.title{
  font-size: 26rpx;
  border-bottom: 1rpx solid #d8dddc;
  height: 85rpx;
  line-height: 85rpx;  
  padding-left: 28rpx;
  background: #f3f5f7;
}
.text-title{
  display: flex;
  justify-content: flex-start;
   color: #666;
 margin-left: 65rpx;
   font-size: 26rpx;
}
.text-right{
  display: flex;
  justify-content: flex-end;
text-align: center;
  padding-left: 40rpx;
  padding-right: 100rpx;
  color: #666;
  font-size: 26rpx;
}

.next__btn{
  background: #008E40;
  color: #fff;
  margin: 50rpx 0 0 50rpx;
  width: 88%;
  font-size: 33rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.next__btn:last-child{
  margin-bottom: 100rpx;
}
.sms{
  position: absolute;
  top: 1rpx;
  right: 10rpx;
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: block;
  color: #fff;
  background: #008E40;
  text-align: center;
  z-index: 9;
  font-size: 24rpx;
  border-radius: 0 10rpx 10rpx 0;
}
.checkbox{
  margin-top: 20rpx;
  font-size: 26rpx;
  padding-left: 40rpx;
}
.details__info{
  display: flex;
  justify-content: space-between;
  position: relative;
  margin-right: 10rpx;
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  border-bottom: 1rpx solid #d8dddc;
  color: #666;
  font-size: 26rpx;
  padding-left: 28rpx;
  background: #fff;
}
.radio-group{
  padding: 15rpx 0 15px 28rpx;
  border-bottom: 1rpx solid #d8dddc;
  background:#fff;
}
.details__info1{
  display: flex;
  justify-content: space-between;
  position: relative;
  margin-right: 10rpx;
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  border-bottom: 1rpx solid #d8dddc;
  color: #666;
  font-size: 26rpx;
  padding-left: 28rpx;
  margin-top: 30rpx;
  background: #fff;
}
.logo__text{
  display: flex;
  justify-content: flex-start;
}
.radio__text{
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;  
  line-height: 65rpx;
  vertical-align: middle;
}
.time{
  display: flex;
  justify-content: flex-end;
  padding: 4rpx 40rpx 0 0;
}
.item{
    font-size: 30rpx;
    color: #5f5f5f;
    border-bottom: 1rpx solid #e0e1e1;
    background: #ffffff;
}
.item__name{
    height: 75rpx;
    display: flex;
    align-items: center;
    position: relative;
    padding: 0;
    margin-left:20rpx;
    margin-right:20rpx;
    border-bottom: 1rpx solid #e0e1e1;
    font-size: 26rpx;
}
.item__content{
    padding: 25rpx 20rpx;
}
.text--val{
    font-size: 24rpx;
    line-height: 40rpx;
    color: #686868;
}

.text--status{
    font-size: 26rpx;
    float: right;
    color: #fb972c;
    margin-right: 10rpx;
}
.text-bold{
  padding-right: 20rpx;
}