.save__btn {
  background: #008e40;
  color: #fff;
  margin-top: 30rpx;
  width: 94%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}

.data__box:after {
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  border: 3rpx solid #8c8e91;
  border-left: 0;
  border-bottom: 0;
  transform: rotate(45deg);
  position: absolute;
  right: 0;
  top: 30rpx;
  z-index: 1;
}
