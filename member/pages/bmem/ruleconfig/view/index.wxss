.data {
  background: #fff;
  display: flex;
  justify-content: space-between;
}

.bill__data {
  display: flex;
  justify-content: flex-start;
}

.data__box{
  display: flex;
  justify-content: flex-end;
  position: relative;
  right: 50rpx;
  padding-right: 30rpx;
  color: #333;
}

